{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "types": [
      "bun-types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
  },
}