{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "types": [
      "bun-types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/layouts/*": [
        "src/layouts/*"
      ]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
  },
  "exclude": [
    "node_modules",
    "dist",
    ".history"
  ]
}